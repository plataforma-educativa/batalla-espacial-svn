<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?scenebuilder-preview-i18n-resource contenido.properties?>

<fx:root type="javafx.scene.layout.BorderPane" mouseTransparent="false" pickOnBounds="true" prefHeight="590.0" prefWidth="740.0" style="&#10;" stylesheets="ar/com/comunidadesfera/batallaespacial/interfaz/estilos.css" xmlns:fx="http://javafx.com/fxml" fx:controller="ar.com.comunidadesfera.batallaespacial.interfaz.ControladorPrincipal">
  <center>
    <TabPane cache="false" mouseTransparent="false" pickOnBounds="false" prefHeight="200.0" prefWidth="200.0" side="TOP" tabClosingPolicy="UNAVAILABLE" visible="true">
      <tabs>
        <Tab text="%seccion.partida">
          <content>
            <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0">
              <center>
                <ScrollPane fx:id="panelMarcoTablero" content="$null" prefHeight="200.0" prefWidth="200.0">
                  <BorderPane.margin>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" fx:id="x1" />
                  </BorderPane.margin>
                </ScrollPane>
              </center>
              <padding>
                <Insets />
              </padding>
              <right>
                <fx:include fx:id="panelInformes" source="informes/informes.fxml" />
              </right>
            </BorderPane>
          </content>
        </Tab>
        <Tab id="" text="%seccion.laboratorio">
          <content>
            <AnchorPane id="AnchorPane" prefHeight="530.0" prefWidth="740.0">
              <children>
                <TabPane id="" disable="false" prefHeight="200.0" prefWidth="200.0" side="TOP" styleClass="floating" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="0.0">
                  <tabs>
                    <Tab text="%ordenamiento.titulo">
                      <content>
                        <fx:include fx:id="panelOrdenamiento" source="laboratorios/ordenamiento/contenedores.fxml" />
                      </content>
                    </Tab>
                    <Tab text="%busqueda.titulo">
                      <content>
                        <fx:include fx:id="panelBusqueda" source="laboratorios/busqueda/piezas.fxml" />
                      </content>
                    </Tab>
                  </tabs>
                </TabPane>
              </children>
            </AnchorPane>
          </content>
        </Tab>
        <Tab closable="false" text="%seccion.registros">
          <content>
            <fx:include fx:id="panelEficiencia" source="eficiencia/mediciones.fxml" />
          </content>
        </Tab>
        <Tab text="%seccion.configuracion">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
          </content>
        </Tab>
      </tabs>
      <BorderPane.margin>
        <Insets />
      </BorderPane.margin>
    </TabPane>
  </center>
  <top>
    <MenuBar>
      <menus>
        <Menu mnemonicParsing="false" text="%menu.partida">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#nuevaPartida" text="%menu.partida.nueva" />
            <MenuItem mnemonicParsing="false" text="%menu.partida.salir" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="%menu.metricas">
          <items>
            <CheckMenuItem mnemonicParsing="false" text="%menu.metricas.registrar" fx:id="menuMetricasRegistrar" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="%menu.ayuda">
          <items>
            <MenuItem mnemonicParsing="false" text="%menu.ayuda.acercaDe" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
  </top>
</fx:root>
